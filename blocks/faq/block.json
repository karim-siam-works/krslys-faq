{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "next-level-faq/faq",
  "title": "Next Level FAQ",
  "category": "widgets",
  "icon": "editor-help",
  "description": "Display a styled FAQ section using the Next Level FAQ styles.",
  "keywords": ["faq", "accordion", "help"],
  "textdomain": "next-level-faq",
  "supports": {
    "html": false,
    "align": true,
    "anchor": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "default": "Frequently Asked Questions"
    },
    "groupId": {
      "type": "number",
      "default": 0
    }
  },
  "editorScript": "nlf-faq-block-editor",
  "style": "nlf-faq-generated"
}


